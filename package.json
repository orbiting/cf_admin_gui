    "d3-format": "^1.3.2",
    "d3-time-format": "^2.1.3",
    "downshift": "^2.2.2",
    "express": "^4.16.4",
    "express-basic-auth": "^1.2.0",
    "glamor": "^2.20.40",
    "graphql": "^14.0.2",
